  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"// Background stars\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float a = pow((1.0-dist), 20.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float t = time*-0.05;\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float r = coord.x - (t*SPEED);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float c = fract(a+coord.y + 0.0*0.543);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"vec2  p = vec2(r, c*0.5)*4000.0;\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"vec2 uv = fract(p)*2.0-1.0;\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float m = clamp((rand(floor(p))-0.9)*BRIGHTNESS, 0.0, 1.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"color +=  clamp((1.0-length(uv*2.0))*m*dist, 0.0, 1.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"// Flying stars into black hole\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"for (float i = 1.0; i < (LAYERS+1.0); ++i)\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"{\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float a = pow((1.0-dist),20.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float t = i*10.0 + time*i*i;\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float r = coord.x - (t*SPEED);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float c = fract(a+coord.y + i*.543);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"vec2  p = vec2(r, c*.5)*SCALE*(LAYERS/(i*i));\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"vec2 uv = fract(p)*2.0-1.0;\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"float m = clamp((rand(floor(p))-DENSITY/i)*BRIGHTNESS, 0.0, 1.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"color +=  clamp(star(uv*0.5, time+i*10.0)*m*dist, 0.0, 1.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"}\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"gl_FragColor = vec4(color, 1.0);\""
  },
  {
    "type": "Punctuator",
    "value": ","
  },
  {
    "type": "String",
    "value": "\"}\""
